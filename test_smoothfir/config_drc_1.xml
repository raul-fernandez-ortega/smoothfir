<main>
<crap></crap>
<smoothfir>
  <globals>
    <sampling_rate>44100</sampling_rate>
    <overflow_warnings>true</overflow_warnings>
    <filter_length>
      <block_length>512</block_length>
      <n_blocks>64</n_blocks>
    </filter_length>
    <!--debug>true</debug-->
    <!--powersave>-60</powersave-->
    <!--show_progress>true</show_progress-->
    <logic>
      <name>race</name>
      <params>
	<direct_coeff>race direct</direct_coeff>
	<cross_coeff>race cross</cross_coeff>
	<attenuation>6.0</attenuation>
	<delay>4</delay>
	<low_cut_freq>200</low_cut_freq>
	<low_cut_slope>24</low_cut_slope>
	<high_cut_freq>6000</high_cut_freq>
	<high_cut_slope>24</high_cut_slope>
      </params>
    </logic>
  </globals>
  <coeff>
    <name>race direct</name>
    <filename>dirac pulse</filename>
    <blocks>8</blocks>
  </coeff>
  <coeff>
    <name>race cross</name>
    <filename>dirac pulse</filename>
    <blocks>8</blocks>
  </coeff>
  <coeff>
    <name>DRC00_left</name>
    <filename>/mnt/sda1/Measurements/project_0/left/drc_0/DRC.wav</filename>
    <format>sndfile</format>
    <shared_mem>true</shared_mem>
    <blocks>64</blocks>
  </coeff>
  <coeff>
    <name>DRC00_right</name>
    <filename>/mnt/sda1/Measurements/project_0/right/drc_0/DRC.wav</filename>
    <format>sndfile</format>
    <shared_mem>true</shared_mem>
    <blocks>64</blocks>
  </coeff>
  <coeff>
    <name>DRC01_left</name>
    <filename>/mnt/sda1/Measurements/project_0/left/drc_1/DRC.wav</filename>
    <format>sndfile</format>
    <shared_mem>true</shared_mem>
    <blocks>64</blocks>
  </coeff>
  <coeff>
    <name>DRC01_right</name>
    <filename>/mnt/sda1/Measurements/project_0/right/drc_1/DRC.wav</filename>
    <format>sndfile</format>
    <shared_mem>true</shared_mem>
    <blocks>64</blocks>
  </coeff>
  <coeff>
    <name>DRC02_left</name>
    <filename>/mnt/sda1/Measurements/project_0/left/drc_2/DRC.wav</filename>
    <format>sndfile</format>
    <shared_mem>true</shared_mem>
    <blocks>64</blocks>
  </coeff>
  <coeff>
    <name>DRC02_right</name>
    <filename>/mnt/sda1/Measurements/project_0/right/drc_2/DRC.wav</filename>
    <format>sndfile</format>
    <shared_mem>true</shared_mem>
    <blocks>64</blocks>
  </coeff>
  <coeff>
    <name>DRC03_left</name>
    <filename>/mnt/sda1/Measurements/project_0/left/drc_3/DRC.wav</filename>
    <format>sndfile</format>
    <shared_mem>true</shared_mem>
    <blocks>64</blocks>
  </coeff>
  <coeff>
    <name>DRC03_right</name>
    <filename>/mnt/sda1/Measurements/project_0/right/drc_3/DRC.wav</filename>
    <format>sndfile</format>
    <shared_mem>true</shared_mem>
    <blocks>64</blocks>
  </coeff>
  <input> 
    <params>
      <priority>65</priority>
      <port>
	<index>0</index>
	<destname>system:capture_3</destname>
      </port>
      <port>
	<index>1</index>
	<destname>system:capture_4</destname>
      </port>
    </params>
    <channel>
      <name>left</name>
      <mapping>0</mapping>
      <delay>0</delay>
    </channel>
    <channel>
      <name>right</name>
      <mapping>1</mapping>
      <delay>0</delay>
    </channel>
  </input>
  <output> 
    <params>
      <port>
	<index>0</index>
	<destname>system:playback_7</destname>
      </port>
      <port>
	<index>1</index>
	<destname>system:playback_8</destname>
      </port>
    </params>
    <channel>
      <name>left</name>
      <mapping>0</mapping>
      <!--delay>0</delay-->
    </channel>
    <channel>
      <name>right</name>
      <mapping>1</mapping>
      <!--delay>0</delay-->
    </channel>
  </output>
  <filter>
    <name>DRC_left</name>
    <coeff_name>DRC02_left</coeff_name>
    <from_inputs>
      <name>left</name>
      <attenuation>0.0</attenuation>
    </from_inputs>
    <to_outputs>
      <name>left</name>
      <attenuation>0.0</attenuation>
    </to_outputs>
  </filter>
  <filter>
    <name>DRC_right</name>
    <coeff_name>DRC02_right</coeff_name>
    <from_inputs>
      <name>right</name>
      <attenuation>0.0</attenuation>
    </from_inputs>
    <to_outputs>
      <name>right</name>
      <attenuation>0.0</attenuation>
    </to_outputs>
  </filter>
</smoothfir>
</main>
