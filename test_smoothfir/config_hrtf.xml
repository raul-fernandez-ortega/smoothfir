<main>
<crap></crap>
<brutefir>
  <globals>
    <sampling_rate>44100</sampling_rate>
    <filter_length>
      <block_length>512</block_length>
      <n_blocks>8</n_blocks>
    </filter_length>
    <!--debug>true</debug-->
    <!--powersave>-60</powersave-->
    <!--show_progress>true</show_progress-->
    <convolver_config>/home/raul/.brutefir_config</convolver_config>
    <modules_path>/usr/lib/brutefir</modules_path>
    <logic>
      <name>ladspa</name>
      <params>
	<plugin>
	  <label>buttlow_iir</label>
	  <input>left</input>
	  <port>
	    <name>Cutoff Frequency (Hz)</name>
	    <value>300</value>
	  </port>
	  <port>
	    <name>Resonance</name>
	    <value>0,5</value>
	  </port>
	</plugin>
	<plugin>
	  <label>butthigh_iir</label>
	  <output>right</output>
	  <port>
	    <name>Cutoff Frequency (Hz)</name>
	    <value>300</value>
	  </port>
	<port>
	  <name>Resonance</name>
	  <value>0,5</value>
	</port>
	</plugin>
	<plugin>
	  <label>inv</label>
	  <output>right</output>
	</plugin>
      </params>
      <!--name>cli</name>
      <params>
	<port>3000</port>
      </params-->
      <name>eq</name>
      <params>
	<coeff>eq-0</coeff>
	<std>ISO 1/3 octave</std>
	<band><freq>20</freq><mag>0.0</mag><phase>0</phase></band>
	<band><freq>25</freq><mag>0.0</mag><phase>0</phase></band>
	<band><freq>31</freq><mag>0.0</mag><phase>0</phase></band>
	<band><freq>40</freq><mag>0.0</mag><phase>0</phase></band>
	<band><freq>50</freq><mag>0.0</mag><phase>0</phase></band>
	<band><freq>63</freq><mag>0.0</mag><phase>0</phase></band>
	<band><freq>80</freq><mag>0.0</mag><phase>0</phase></band>
	<band><freq>100</freq><mag>0.0</mag><phase>0</phase></band>
	<band><freq>125</freq><mag>0.0</mag><phase>0</phase></band>
	<band><freq>160</freq><mag>0.0</mag><phase>0</phase></band>
	<band><freq>200</freq><mag>0.0</mag><phase>0</phase></band>
	<band><freq>250</freq><mag>0.0</mag><phase>0</phase></band>
	<band><freq>315</freq><mag>0.0</mag><phase>0</phase></band>
	<band><freq>400</freq><mag>0.0</mag><phase>0</phase></band>
	<band><freq>500</freq><mag>0.0</mag><phase>0</phase></band>
	<band><freq>630</freq><mag>0.0</mag><phase>0</phase></band>
	<band><freq>800</freq><mag>0.0</mag><phase>0</phase></band>
	<band><freq>1000</freq><mag>-5.0</mag><phase>0</phase></band>
	<band><freq>1250</freq><mag>0.0</mag><phase>0</phase></band>
	<band><freq>1600</freq><mag>0.0</mag><phase>0</phase></band>
	<band><freq>2000</freq><mag>0.0</mag><phase>0</phase></band>
	<band><freq>2500</freq><mag>0.0</mag><phase>0</phase></band>
	<band><freq>3150</freq><mag>0.0</mag><phase>0</phase></band>
	<band><freq>4000</freq><mag>0.0</mag><phase>0</phase></band>
	<band><freq>5000</freq><mag>0.0</mag><phase>0</phase></band>
	<band><freq>6300</freq><mag>0.0</mag><phase>0</phase></band>
	<band><freq>8000</freq><mag>0.0</mag><phase>0</phase></band>
	<band><freq>10000</freq><mag>0.0</mag><phase>0</phase></band>
	<band><freq>12500</freq><mag>0.0</mag><phase>0</phase></band>
	<band><freq>16000</freq><mag>0.0</mag><phase>0</phase></band>
	<band><freq>20000</freq><mag>0.0</mag><phase>0</phase></band>
      </params>
    </logic>
  </globals>
  <coeff>
    <name>left_hrtf</name>
    <filename>/home/raul/acustica/hrtf/setup/RAW/WAV/IRC_1002_R/IRC_1002_R_R0195_T000_P000_L.raw</filename>
    <format>FLOAT_LE</format>
    <channel>0</channel>
    <attenuation>10.0</attenuation>
  </coeff>
  <coeff>
    <name>right_hrtf</name>
    <filename>/home/raul/acustica/hrtf/setup/RAW/WAV/IRC_1002_R/IRC_1002_R_R0195_T000_P000.wav</filename>
    <format>sndfile</format>
    <channel>1</channel>
    <attenuation>10.0</attenuation>
  </coeff>
  <coeff>
    <name>eq-0</name>
    <filename>dirac pulse</filename>
    <shared_mem>true</shared_mem>
  </coeff>
  <input> 
    <device>jack</device>
    <device_params>
      <priority>65</priority>
      <port>
	<index>0</index>
	<destname>system:capture_1</destname>
      </port>
      <port>
	<index>1</index>
	<destname>system:capture_2</destname>
      </port>
    </device_params>
    <sample>FLOAT_LE</sample>
    <channel>
      <name>left</name>
      <mapping>0</mapping>
      <delay>0</delay>
    </channel>
    <channel>
      <name>right</name>
      <mapping>1</mapping>
      <delay>0</delay>
    </channel>
    <channel>
      <name>right2</name>
      <mapping>1</mapping>
      <delay>0</delay>
    </channel>
  </input>
  <output> 
    <device>jack</device>
    <priority>65</priority>
    <device_params>
      <port>
	<index>0</index>
	<destname>system:playback_1</destname>
      </port>
      <port>
	<index>1</index>
	<destname>system:playback_2</destname>
      </port>
    </device_params>
    <sample>FLOAT_LE</sample>
    <open_channels>2</open_channels>
    <channel>
      <name>left</name>
      <mapping>0</mapping>
      <delay>0</delay>
    </channel>
    <channel>
      <name>left2</name>
      <mapping>0</mapping>
      <delay>0</delay>
    </channel>
    <channel>
      <name>right</name>
      <mapping>1</mapping>
      <delay>0</delay>
    </channel>
  </output>
  <filter>
    <name>left speaker direct path</name>
    <coeff_name>eq-0</coeff_name>
    <from_inputs>
      <name>left</name>
      <attenuation>0.0</attenuation>
    </from_inputs>
    <to_outputs>
      <name>left</name>
      <attenuation>0.0</attenuation>
      <name>left2</name>
      <attenuation>0.0</attenuation>
    </to_outputs>
  </filter>
  <filter>
    <name>right speaker 2 direct path</name>
    <coeff_name>dirac pulse</coeff_name>
    <from_inputs>
      <name>right2</name>
      <attenuation>0.0</attenuation>
    </from_inputs>
    <to_filters>
      <name>right speaker direct path</name>
    </to_filters>
  </filter>
  <filter>
    <name>right speaker direct path</name>
    <coeff_name>right_hrtf</coeff_name>
    <from_filters>
      <name>right speaker 2 direct path</name>
    </from_filters>
    <to_outputs>
      <name>right</name>
      <attenuation>0.0</attenuation>
    </to_outputs>
  </filter>
</brutefir>
</main>
