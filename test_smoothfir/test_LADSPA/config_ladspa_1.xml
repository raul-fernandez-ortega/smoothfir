<main>
<crap></crap>
<smoothfir>
  <globals>
    <sampling_rate>44100</sampling_rate>
    <overflow_warnings>true</overflow_warnings>
    <filter_length>
      <block_length>512</block_length>
      <n_blocks>8</n_blocks>
    </filter_length>
    <!--debug>true</debug-->
    <!--powersave>-60</powersave-->
    <!--show_progress>true</show_progress-->
    <logic>
      <name>ladspa</name>
      <params>
	<plugin>
	  <label>buttlow_iir</label>
	  <input>left</input>
	  <port>
	    <name>Cutoff Frequency (Hz)</name>
	    <value>300</value>
	  </port>
	  <port>
	    <name>Resonance</name>
	    <value>0.5</value>
	  </port>
	</plugin>
	<plugin>
	  <label>butthigh_iir</label>
	  <output>right</output>
	  <port>
	    <name>Cutoff Frequency (Hz)</name>
	    <value>300</value>
	  </port>
	  <port>
	    <name>Resonance</name>
	    <value>0.5</value>
	  </port>
	</plugin>
	<plugin>
	  <label>inv</label>
	  <output>right</output>
	  <port></port>
	</plugin>
      </params>
    </logic>
  </globals>
  <coeff>
    <name>dirac</name>
    <filename>dirac pulse</filename>
    <blocks>8</blocks>
  </coeff>
  <input> 
    <params>
      <priority>65</priority>
      <port>
	<index>0</index>
	<destname>system:capture_1</destname>
      </port>
      <port>
	<index>1</index>
	<destname>system:capture_2</destname>
      </port>
    </params>
    <channel>
      <name>left</name>
      <mapping>0</mapping>
      <delay>0</delay>
    </channel>
    <channel>
      <name>right</name>
      <mapping>1</mapping>
      <delay>0</delay>
    </channel>
  </input>
  <output> 
    <priority>65</priority>
    <params>
      <port>
	<index>0</index>
	<destname>system:playback_1</destname>
      </port>
      <port>
	<index>1</index>
	<destname>system:playback_2</destname>
      </port>
    </params>
    <channel>
      <name>left</name>
      <mapping>0</mapping>
      <delay>0</delay>
    </channel>
    <channel>
      <name>right</name>
      <mapping>1</mapping>
      <delay>0</delay>
    </channel>
  </output>
  <filter>
    <name>left</name>
    <coeff_name>dirac</coeff_name>
    <from_inputs>
      <name>left</name>
      <attenuation>0.0</attenuation>
    </from_inputs>
    <to_outputs>
      <name>left</name>
      <attenuation>0.0</attenuation>
    </to_outputs>
  </filter>
  <filter>
    <name>right</name>
    <coeff_name>dirac</coeff_name>
    <from_inputs>
      <name>right</name>
      <attenuation>0.0</attenuation>
    </from_inputs>
    <to_outputs>
      <name>right</name>
      <attenuation>0.0</attenuation>
    </to_outputs>
  </filter>
</smoothfir>
</main>
