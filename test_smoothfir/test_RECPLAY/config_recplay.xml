<main>
<crap></crap>
<smoothfir>
  <globals>
    <sampling_rate>44100</sampling_rate>
    <overflow_warnings>true</overflow_warnings>
    <filter_length>
      <block_length>512</block_length>
      <n_blocks>8</n_blocks>
    </filter_length>
  </globals>
  <coeff>
    <name>left</name>
    <filename>dirac pulse</filename>
    <blocks>8</blocks>
  </coeff>
  <coeff>
    <name>right</name>
    <filename>dirac pulse</filename>
    <blocks>8</blocks>
  </coeff>
  <input> 
    <params>
      <priority>65</priority>
      <port>
	<index>0</index>
	<destname>system:capture_1</destname>
      </port>
      <port>
	<index>1</index>
	<destname>system:capture_2</destname>
      </port>
    </params>
    <channel>
      <name>left</name>
      <mapping>0</mapping>
      <delay>0</delay>
    </channel>
    <channel>
      <name>right</name>
      <mapping>1</mapping>
      <delay>0</delay>
    </channel>
  </input>
  <output> 
    <params>
      <port>
	<index>0</index>
	<destname>system:playback_1</destname>
      </port>
      <port>
	<index>1</index>
	<destname>system:playback_2</destname>
      </port>
    </params>
    <channel>
      <name>left</name>
      <mapping>0</mapping>
    </channel>
    <channel>
      <name>right</name>
      <mapping>1</mapping>
    </channel>
  </output>
  <filter>
    <name>left</name>
    <coeff_name>left</coeff_name>
    <from_inputs>
      <name>left</name>
      <attenuation>0.0</attenuation>
    </from_inputs>
    <to_outputs>
      <name>left</name>
      <attenuation>0.0</attenuation>
    </to_outputs>
  </filter>
  <filter>
    <name>right</name>
    <coeff_name>right</coeff_name>
    <from_inputs>
      <name>right</name>
      <attenuation>0.0</attenuation>
    </from_inputs>
    <to_outputs>
      <name>right</name>
      <attenuation>0.0</attenuation>
    </to_outputs>
  </filter>
</smoothfir>
</main>
